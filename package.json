      "import": "./react.js",
      "types": "./react.d.ts"
    }
  },
  "contributors": [
    {
      "name": "Lukas A. Schopf",
      "url": "https://thelaumix.com/"
    }
  ],
  "scripts": {
    "build": "tsc  -d"
  },
