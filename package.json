    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6"
  },
  "dependencies": {
    "lux-callback-emitter": "^1.0.1"
  },
  "homepage": "https://github.com/thelaumix/flex-path",
  "keywords": [
